/dts-v1/;
#include <nordic/nrf52832_qfaa.dtsi>
#include "nrf52dk_nrf52832_CyLock-pinctrl.dtsi"

/ {
	aliases {
		modem = &modem;
		modem-uart = &uart0;
	};
};

/ {
	model = "Nordic nRF52 DK NRF52832";
	compatible = "nordic,nrf52-dk-nrf52832";

	chosen {
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
		zephyr,uart-mcumgr = &uart0;
		zephyr,bt-mon-uart = &uart0;
		zephyr,bt-c2h-uart = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		// zephyr,code-partition = &slot0_partition;
		nordic,pm-ext-flash = &SST26VF016B;
	};


	leds {
		compatible = "gpio-leds";
		led0: led_0 {
			gpios = <&gpio0 22 GPIO_ACTIVE_LOW>;
			label = "Green LED 0";
			// status = "disabled";
		};
		led1: led_1 {
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			label = "Green LED 1";
			status = "disabled";
		};
		led2: led_2 {
			gpios = <&gpio0 19 GPIO_ACTIVE_LOW>;
			label = "Green LED 2";
			status = "disabled";
		};
		led3: led_3 {
			gpios = <&gpio0 20 GPIO_ACTIVE_LOW>;
			label = "Green LED 3";
			status = "disabled";
		};
	};

	pwmleds {
		compatible = "pwm-leds";
		pwm_led0: pwm_led_0 {
			pwms = <&pwm0 0 PWM_MSEC(20) PWM_POLARITY_INVERTED>;
			status = "disabled";
		};
	};

	buttons {
		compatible = "gpio-keys";
		button0: button_0 {
			gpios = <&gpio0 13 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
			label = "Push button switch 0";
			status = "disabled";
		};
		button1: button_1 {
			gpios = <&gpio0 14 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
			label = "Push button switch 1";
			status = "disabled";
		};
		button2: button_2 {
			gpios = <&gpio0 15 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
			label = "Push button switch 2";
			status = "disabled";
		};
		button3: button_3 {
			gpios = <&gpio0 16 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
			label = "Push button switch 3";
			status = "disabled";
		};
	};

	arduino_header: connector {
		compatible = "arduino-header-r3";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <0 0 &gpio0 3 0>,    /* A0 */
		<1 0 &gpio0 4 0>,    /* A1 */
		<2 0 &gpio0 28 0>,    /* A2 */
		<3 0 &gpio0 29 0>,    /* A3 */
		<4 0 &gpio0 30 0>,    /* A4 */
		<5 0 &gpio0 31 0>,    /* A5 */
		// <6 0 &gpio0 11 0>,    /* D0 */
		// <7 0 &gpio0 12 0>,    /* D1 */
		<8 0 &gpio0 13 0>,    /* D2 */
		<9 0 &gpio0 14 0>,    /* D3 */
		<10 0 &gpio0 15 0>,    /* D4 */
		<11 0 &gpio0 16 0>,    /* D5 */
		<12 0 &gpio0 17 0>,    /* D6 */
		<13 0 &gpio0 18 0>,    /* D7 */
		<14 0 &gpio0 19 0>,    /* D8 */
		<15 0 &gpio0 20 0>,    /* D9 */
		<16 0 &gpio0 22 0>,    /* D10 */
		<17 0 &gpio0 23 0>,    /* D11 */
		<18 0 &gpio0 24 0>,    /* D12 */
		<19 0 &gpio0 25 0>,    /* D13 */
		<20 0 &gpio0 26 0>;    /* D14 */
		// <21 0 &gpio0 27 0>;    /* D15 */
	};

	arduino_adc: analog-connector {
		compatible = "arduino,uno-adc";
		#io-channel-cells = <1>;
		io-channel-map = <0 &adc 1>, /* A0 = P0.3 = AIN1 */
		                 <1 &adc 2>, /* A1 = P0.4 = AIN2 */
		                 <2 &adc 4>, /* A2 = P0.28 = AIN4 */
		                 <3 &adc 5>, /* A3 = P0.29 = AIN5 */
		                 <4 &adc 6>, /* A4 = P0.30 = AIN6 */
		                 <5 &adc 7>;    /* A5 = P0.31 = AIN7 */
	};

	/* These aliases are provided for compatibility with samples */
	aliases {
		gpio0 = &gpio0;
		watchdog0 = &wdt0;
		spiflash = &spi1;
		led = &led0;
		adc0 =  &adc;
		uart = &uart0;
		spiflash0 = &SST26VF016B;
	};
};

&adc {
	status = "okay";
};

&gpiote {
	status = "okay";
};

&uicr {
	// gpio-as-nreset;
	nfct-pins-as-gpios;
};


&gpio0 {
	status = "okay";
};

arduino_serial: &uart0 {
	status = "okay";
	compatible = "nordic,nrf-uarte";
	current-speed = <115200>;
	pinctrl-0 = <&uart0_default>;
	pinctrl-1 = <&uart0_sleep>;
	pinctrl-names = "default", "sleep";
	gsm: gsm-modem {
		compatible = "zephyr,gsm-ppp";
		// label = "gsm_ppp";
	};


	modem: modem {
		compatible = "quectel,bg95";
		mdm-power-gpios = <&gpio0 10 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};
	// modem: modem {
		//     compatible = "quectel,bg95";
		//     mdm-power-gpios = <&gpio0 10 GPIO_ACTIVE_HIGH>;
		//     status = "okay";
	// };
};

arduino_i2c: &i2c0 {
	compatible = "nordic,nrf-twi";
	status = "disabled";
	pinctrl-0 = <&i2c0_default>;
	pinctrl-1 = <&i2c0_sleep>;
	pinctrl-names = "default", "sleep";
};

&i2c1 {
	compatible = "nordic,nrf-twi";
	/* Cannot be used together with spi1. */
	status = "disabled";
	pinctrl-0 = <&i2c1_default>;
	pinctrl-1 = <&i2c1_sleep>;
	pinctrl-names = "default", "sleep";
};

&pwm0 {
	status = "okay";
	pinctrl-0 = <&pwm0_default>;
	pinctrl-1 = <&pwm0_sleep>;
	pinctrl-names = "default", "sleep";
};

&spi0 {
	compatible = "nordic,nrf-spi";
	/* Cannot be used together with i2c0. */
	status = "disabled";
	pinctrl-0 = <&spi0_default>;
	pinctrl-1 = <&spi0_sleep>;
	pinctrl-names = "default", "sleep";
};

&spi1 {
	compatible = "nordic,nrf-spi";
	status = "okay";
	pinctrl-0 = <&spi1_default>;
	pinctrl-1 = <&spi1_sleep>;
	pinctrl-names = "default", "sleep";
	cs-gpios = <&gpio0 27 GPIO_ACTIVE_LOW>;
	SST26VF016B: SST26VF016B@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <8000000>;
		requires-ulbpr;
		// label = "SST26VF016B";
		jedec-id = [ BF 26 41 ];
		//sfdp-bfp = [
		//    e5 20 f1 ff  ff ff ff 03  44 eb 08 6b  08 3b 04 bb
		//    ee ff ff ff  ff ff 00 ff  ff ff 00 ff  0c 20 0f 52
		//    10 d8 00 ff  23 72 f5 00  82 ed 04 cc  44 83 68 44
		//    30 b0 30 b0  f7 c4 d5 5c  00 be 29 ff  f0 d0 ff ff
		//];
		size = <16777216>;
		//size = <67108864>;
		status = "okay";
		has-dpd;
		t-enter-dpd = <10000>;
		t-exit-dpd = <35000>;
	};
};

arduino_spi: &spi2 {
	compatible = "nordic,nrf-spi";
	status = "disabled";
	cs-gpios = <&arduino_header 16 GPIO_ACTIVE_LOW>; /* D10 */
	pinctrl-0 = <&spi2_default>;
	pinctrl-1 = <&spi2_sleep>;
	pinctrl-names = "default", "sleep";
};


&flash0 {

	partitions {
		 compatible = "fixed-partitions";
		 #address-cells = <1>;
		 #size-cells = <1>;
 
		boot_partition: partition@0 {
			 label = "mcuboot";
			 reg = <0x00000000 0xD000>;
		 };
		 slot0_partition: partition@d000 {
			 label = "image-0";
			 reg = <0x0000D000 0x31800>;
		 };
		 slot1_partition: partition@3e800 {
			 label = "image-1";
			 reg = <0x0003E800 0x31800>;
		 };
		 scratch_partition: partition@70000 {
			 label = "image-scratch";
			 reg = <0x00070000 0xa000>;
		 };
		 storage_partition: partition@7a000 {
			 label = "storage";
			 reg = <0x0007a000 0x00006000>;
		 };
	 };
 };
 

